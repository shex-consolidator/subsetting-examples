PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX up: <http://purl.uniprot.org/core/>

<protein> {
    a             [ up:Protein ] ;
    up:annotation @<annotation> 
}

<annotation> {
    a [ up:Glycosylation_Annotation ]
}